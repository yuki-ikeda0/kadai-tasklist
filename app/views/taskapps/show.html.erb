<div class="row">
  <div class="col-xs-6">

<h1>タスクの詳細</h1>

 <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>ステータス</th>
        <th>メッセージ</th>
      </tr>
    </thead>
    <tbody>
       <tr>
        <th><%= @taskapp.id %></th>
        <th><%= @taskapp.status %></th>
        <th><%= @taskapp.content %></th>
      </tr>
    </tbody>
  </table>  
    <%= link_to '一覧に戻る', taskapps_path %>
    <%= link_to 'このメッセージを編集する', edit_taskapp_path(@taskapp), class: 'btn btn-default' %>
    <%= link_to 'このメッセージを削除する', @taskapp, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-mx' %>
 
    







